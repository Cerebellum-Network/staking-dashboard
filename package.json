  "license": "apache-2.0",
  "private": false,
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "deep-clean": "yarn clean && rm -rf node_modules",
    "clean": "rm -rf build/ tsconfig.tsbuildinfo",
    "typecheck": "tsc -b",
    "typecheck:watch": "tsc -b --watch",
    "copy-assets": "cp -R ./src/assets ./build",
    "start": "yarn clean && concurrently \"yarn typecheck:watch\" \"webpack serve --config webpack.config.js --env development\"",
    "build": "yarn clean && yarn typecheck && webpack  --env production",
    "test": "jest",
    "lint": "eslint ./src --fix && npx prettier --write ./src",
    "lint:scripts": "npx prettier --write ./.scripts",
    "locale:order": "node ./.scripts/localeOrderKeys.js",
    "locale:validate": "node ./.scripts/localeValidate.js"
  },
